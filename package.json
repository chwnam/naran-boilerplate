{
  "name": "naran-boilerplate",
  "description": "Naran boilerplate code for WordPress plugins/themes.",
  "version": "1.4.2",
  "license": "GPL-2.0-or-later",
  "scripts": {
    "start": "NODE_ENV=development wp-scripts start --output-path=assets/js/dist",
    "start:hot": "NODE_ENV=development wp-scripts start --output-path=assets/js/dist --hot",
    "build": "NODE_ENV=production wp-scripts build --output-path=assets/js/dist",
    "remove:hot-update": "php bin/remove-hot-update.php"
  }
}
